---
layout: base-layout.njk
---


<section class="s2 ">
  <div class="section-wrapper">

    <div class="art-content content">
      <div class="image-container">
        <div .second-div>
          <a href="{{ spotifyLink }}" target="_blank"><img src="images/{{ coverArt }}" class="images"></a>
        </div>

      </div>
    </div>

    <div class="info-container content">
      <div class="info-content ">
        <h2 class="project-name">{{ projectName }}</h2>
        <ul class="credit-list">
        <li>
          <div class="list-wrapper">
            <img class="heart" src="images/heart.svg">
            <h6>{{ credit-1 }}</h6>
          </div>
        </li>
        <li>
          <div class="list-wrapper">
            <img class="heart" src="images/heart.svg">
            <h6>{{ credit-2 }}</h6>
          </div>
        </li>
        <li>
          <div class="list-wrapper">
            <img class="heart" src="images/heart.svg">
            <h6>{{ credit-3 }}</h6>
          </div>
        </li>
        </ul>
      </div>
    </div>

  </div>
</section>
